"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":57,"column":4}},{"generated":{"line":54,"column":2},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":65,"column":3}},{"generated":{"line":56,"column":0},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":6,"column":43}},{"generated":{"line":58,"column":0},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":68,"column":0},"name":"module"},{"generated":{"line":58,"column":6},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":68,"column":6}},{"generated":{"line":58,"column":7},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":68,"column":7},"name":"exports"},{"generated":{"line":58,"column":14},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":68,"column":0}},{"generated":{"line":58,"column":17},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":68,"column":17},"name":"CountOperation"},{"generated":{"line":58,"column":31},"source":"node_modules/mongodb/lib/operations/count.js","original":{"line":68,"column":0}}],"sources":{"node_modules/mongodb/lib/operations/count.js":"'use strict';\n\nconst buildCountCommand = require('./common_functions').buildCountCommand;\nconst OperationBase = require('./operation').OperationBase;\n\nclass CountOperation extends OperationBase {\n  constructor(cursor, applySkipLimit, options) {\n    super(options);\n\n    this.cursor = cursor;\n    this.applySkipLimit = applySkipLimit;\n  }\n\n  execute(callback) {\n    const cursor = this.cursor;\n    const applySkipLimit = this.applySkipLimit;\n    const options = this.options;\n\n    if (applySkipLimit) {\n      if (typeof cursor.cursorSkip() === 'number') options.skip = cursor.cursorSkip();\n      if (typeof cursor.cursorLimit() === 'number') options.limit = cursor.cursorLimit();\n    }\n\n    // Ensure we have the right read preference inheritance\n    if (options.readPreference) {\n      cursor.setReadPreference(options.readPreference);\n    }\n\n    if (\n      typeof options.maxTimeMS !== 'number' &&\n      cursor.cmd &&\n      typeof cursor.cmd.maxTimeMS === 'number'\n    ) {\n      options.maxTimeMS = cursor.cmd.maxTimeMS;\n    }\n\n    let finalOptions = {};\n    finalOptions.skip = options.skip;\n    finalOptions.limit = options.limit;\n    finalOptions.hint = options.hint;\n    finalOptions.maxTimeMS = options.maxTimeMS;\n\n    // Command\n    finalOptions.collectionName = cursor.namespace.collection;\n\n    let command;\n    try {\n      command = buildCountCommand(cursor, cursor.cmd.query, finalOptions);\n    } catch (err) {\n      return callback(err);\n    }\n\n    // Set cursor server to the same as the topology\n    cursor.server = cursor.topology.s.coreTopology;\n\n    // Execute the command\n    cursor.topology.command(\n      cursor.namespace.withCollection('$cmd'),\n      command,\n      cursor.options,\n      (err, result) => {\n        callback(err, result ? result.result.n : null);\n      }\n    );\n  }\n}\n\nmodule.exports = CountOperation;\n"},"lineCount":null}},"error":null,"hash":"818e65f96e94e093d237647c058ecb93","cacheData":{"env":{}}}